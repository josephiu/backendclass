<header id="top-header">
    <span class="siteName">
        <a href="/" title = "Return to Home Page">CSE Motors</a>
    </span>
        <div id="tools" class="tools">
        <% if (locals.loggedin) { %>
            <a title="Click to see account info" href="/account/"
                class="acct-link">Welcome <%-
                    locals.accountData.account_firstname %></a>
            <a title="Click to log out" href="/account/logout"
                class="acct-link">Log Out</a>
            <% } else { %>
                <a title="Click to login" href="/account/login"
                    class="acct-link">My Account</a>
                <% } %>
    </div>


</header>